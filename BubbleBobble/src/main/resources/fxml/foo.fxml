<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="359.0" fitWidth="622.0">
         <image>
            <Image url="@../image/ui/gameScene/popUp/confirmExit.png" />
         </image>
      </ImageView>
      <ImageView fx:id="confirmBack" layoutX="39.0" layoutY="269.0" styleClass="Button">
         <image>
            <Image url="@../image/ui/gameScene/popUp/backToGame.png" />
         </image>
      </ImageView>
      <ImageView fx:id="confirmExit" layoutX="400.0" layoutY="269.0" styleClass="Button">
         <image>
            <Image url="@../image/ui/gameScene/popUp/exitGame.png" />
         </image>
      </ImageView>
      <ImageView layoutX="218.0" layoutY="195.0" onMouseClicked="#clickSaveGame" styleClass="Button">
         <image>
            <Image url="@../image/ui/gameScene/popUp/saveGame.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
